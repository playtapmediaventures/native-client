{
  "name": "native-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "serve -s build",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "deploy:staging": "node ./scripts/deploy.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "dependencies": {
    "acorn": "^6.3.0",
    "axios": "^0.18.1",
    "axios-jsonp": "^1.0.2",
    "babel-loader": "^8.0.6",
    "kleur": "^3.0.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-in-viewport": "0.0.37",
    "react-resize-detector": "^3.4.0",
    "react-scripts": "3.0.1",
    "react-tracking": "^7.1.0",
    "typescript": "^3.6.2",
    "ycb": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/react": "^5.1.11",
    "aws-sdk": "^2.521.0",
    "compression-webpack-plugin": "^2.0.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "mime-types": "^2.1.24",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  }
}
